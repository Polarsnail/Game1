
public class inventoryPouch {
	public static String pouch1Name = "Empty";
	public static String pouch2Name = "Empty";
	public static String pouch3Name = "Empty";
	public static String pouch4Name = "Empty";
	public static String pouch5Name = "Empty";
	static void getInvent(){
		
		if(pouch1Name == "Empty" && battleSimulator.battleEnded2 == true){
			pouch1Name = lootSystem.lootPicked;
			battleSimulator.battleEnded2 = false;
			return;		
		}
		else if(pouch2Name == "Empty" && battleSimulator.battleEnded2 == true){
			pouch2Name = lootSystem.lootPicked;
			battleSimulator.battleEnded2 = false;
			return;		
		}
		else if(pouch3Name == "Empty" && battleSimulator.battleEnded2 == true){
			pouch3Name = lootSystem.lootPicked;
			battleSimulator.battleEnded2 = false;
			return;		
		}
		else if(pouch4Name == "Empty" && battleSimulator.battleEnded2 == true){
			pouch1Name = lootSystem.lootPicked;
			battleSimulator.battleEnded2 = false;
			return;		
		}
		else if(pouch5Name == "Empty" && battleSimulator.battleEnded2 == true){
			pouch1Name = lootSystem.lootPicked;
			battleSimulator.battleEnded2 = false;
			return;		
		}
	
	}
}
